<!DOCTYPE html>
<html>
<head>
  <title>Graphdeck</title>
  <%= stylesheet_link_tag :all %>
  <%= javascript_include_tag :defaults %>
  <%= csrf_meta_tag %>
</head>
<body>
  <% if flash[:notice] %>
    <div id="notice">
      <%= flash[:notice] %>
    </div>
  <% end %>
  
  <div id="yield">
    <%= yield %>
  </div>

</body>
</html>
